{
  "name": "@learningalbum/env-validation",
  "version": "1.1.0",
  "description": "Validate environment config",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "dev": "tsc --watch",
    "docs:build": "rm -rf docs && npx typedoc --excludePrivate --excludeProtected --excludeInternal src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gblair/lib-env-validation.git"
  },
  "author": "Gabe Blair",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "jest": "^29.7.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.2.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "typedoc": "^0.26.7"
  }
}
